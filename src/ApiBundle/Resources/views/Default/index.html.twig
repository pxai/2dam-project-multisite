
<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}Multisite{% endblock title %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% stylesheets
    'bundles/api/css/bootstrap.min.css'
    'bundles/api/css/bootstrap-theme.min.css'
    filter='cssrewrite' output='css/compiled/app.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% block head %}{% endblock %}

</head>

<body>
<div class="container">
    {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
        <a class="link" href="{{ path('logout') }}">Logout</a>
        <div><a href="{{ path('chatgroup_index') }}">Chat Groups</a></div>
    {% else %}
        <a class="link" href="{{ path('login') }}">Login</a>
    {% endif %}

    {% block body %}{% endblock body %}

    <h1>Multisite - API</h1>
    <div><a href="http://bizgen.co/web/admin/api/idea">bizgen.co</a></div>
    <div><a href="http://h.eartbe.at/web/admin/api/beat">h.eartbe.at</a></div>
    <div><a href="http://classapp.org/web/admin/api/class">classapp.org</a></div>
    <div><a href="http://wzw.io/web/admin/api/meetup">wzw.io</a></div>
    <div><a href="http://zterr.org/web/admin/api/event">zterr.org</a></div>
    <hr>
    <div><a href="{{ path('item_index') }}">Admin Items</a></div>
    <div><a href="{{ path('chatgroup_index') }}">Chat Groups</a></div>
</div>
{% block javascripts %}
    <!-- JavaScript plugins (requires jQuery) -->
    <!--script src="//code.jquery.com/jquery.min.js"></script-->
    <script src="https://use.fontawesome.com/09c458bfbf.js"></script>

    <!-- Include all compiled plugins (below), or include individual files as needed -->
    {% javascripts
    '@ApiBundle/Resources/public/js/bootstrap.min.js'
    '@ApiBundle/Resources/public/js/jquery.js' %}
    <!-- Optionally enable responsive features in IE8 -->
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}


    {# % javascripts '@CuatrovientosArteanBundle/Resources/public/js/respond.js' % }
        <!-- Optionally enable responsive features in IE8 -->
        <script src="{{ asset_url }}"></script>
     {  %endjavascripts % #}
{% endblock %}
{% block customjavascripts %}
{% endblock customjavascripts %}
</body>
</html>

